{
  "name": "GNSS Tacker Sample",
  "description": "An app that starts a ZED and sends GNSS data.",
  "release": {
    "name": "1.0.0",
    "description": "Sample that sends GNSS data.",
    "type": "docker-compose",
    "default_parameters": {
      "requested": {
        "core": { "disable_app": false },
        "dataFreq": 1,
        "$dataFreq": {
          "name": "Data frequency",
          "order": 5,
          "group": "Data",
          "type": "number",
          "min": 1,
          "max": 3600,
          "step": 1,
          "unit": "seconds",
          "description": "Data frequency defined how often data is generated by the app. Every second by default"
        }
      }
    }
  },
  "permissions": {
    "camera": true
  },
  "auto_install": false,
  "restart_policy": "always"
}
